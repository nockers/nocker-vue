{
  "name": "nocker-vue",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @nocker/vue dev",
    "publish:vue": "yarn workspace @nocker/vue publish --access=public",
    "refactor": "yarn refactor:eslint && yarn refactor:prettier",
    "refactor:eslint": "eslint --fix packages/*/lib/**/*.{ts,tsx}",
    "refactor:prettier": "prettier --write packages/*/lib/**/*.{ts,tsx}",
    "reset": "rm -rf packages/*/dist",
    "www": "serve www -l 1234"
  },
  "resolutions": {
    "jest-mock": "^28.1.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.8",
    "@storybook/addon-essentials": "6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "6.5.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.1.36",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/testing-library": "^0.0.11",
    "@storybook/vue3": "^6.5.8",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^28.1.1",
    "node-fetch": "3.2",
    "npm-check-updates": "^13.1.2",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "serve": "^13.0.2",
    "storybook-tailwind-dark-mode": "^1.0.12",
    "tailwindcss": "^3.1.0",
    "typescript": "^4.7.3",
    "vite": "^3.0.0-alpha.8"
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.18"
  },
  "engines": {
    "node": "16.15.0"
  }
}
